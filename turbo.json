{
 "$schema": "https://turbo.build/schema.json",
 "ui": "tui",
 "tasks": {
  "build": {
   "dependsOn": [
    "^build"
   ],
   "inputs": [
    "$TURBO_DEFAULT$",
    ".env*"
   ],
   "outputs": [
    ".next/**",
    "!.next/cache/**"
   ]
  },
  "lint": {
   "dependsOn": [
    "^lint"
   ]
  },
  "dev": {
   "cache": false,
   "persistent": true
  },
  "pipeline": {
   "build": {
    "env": [
     "AUTH_GOOGLE_ID",
     "AUTH_GOOGLE_SECRET"
    ],
    "outputs": [
     "dist/**"
    ]
   }
  }
 }
}
